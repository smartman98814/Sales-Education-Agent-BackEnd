{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx --watch server.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint \"{config,models,routes,services,utils}/**/*.ts\" --fix",
    "format": "prettier --write \"{config,models,routes,services,utils}/**/*.(js|mjs|cjs|jsx|ts|tsx|vue|css|scss|sass|html|json|json5|jsonc|yml|yaml|md|markdown|graphql|gql|ng)\"",
    "pre-commit": "lint-staged",
    "prepare": "husky",
    "create-agent-index": "tsc && node dist/scripts/createAgentNumberIndex.js",
    "create-all-indexes": "npm run create-agent-index"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.911.0",
    "@aws-sdk/s3-presigned-post": "^3.911.0",
    "@types/cookie-parser": "^1.4.9",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "cheerio": "^1.1.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "form-data": "^4.0.2",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.11.0",
    "mime-types": "^3.0.1",
    "mongodb": "^6.20.0",
    "multer": "^2.0.0",
    "node-cron": "^4.2.1",
    "openai": "^5.23.0",
    "path": "^0.12.7",
    "pdf-parse": "^2.4.5",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/cors": "^2.8.18",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.17.31",
    "@types/form-data": "^2.2.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mime-types": "^3.0.1",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.21",
    "@types/node-cron": "^3.0.11",
    "@types/pdf-parse": "^1.1.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.0.0",
    "prettier-plugin-prisma": "^5.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,jsx,ts,tsx,vue}": [
      "prettier --write",
      "eslint --fix"
    ],
    "**/*.{css,scss,sass,html,json,json5,jsonc,yml,yaml,md,markdown,graphql,gql,ng}": [
      "prettier --write"
    ]
  }
}
